<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

<RelativeLayout
    android:id="@+id/eventResults"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
    android:id="@+id/artist"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignParentStart="true"
    android:layout_alignParentLeft="true"
    android:layout_alignParentTop="true"
    android:layout_marginStart="29dp"
    android:layout_marginLeft="29dp"
    android:layout_marginTop="30dp"
    android:text="Artist/Team(s)"
    android:textSize="16sp"
    android:textStyle="bold"/>

    <TextView
        android:id="@+id/artistVal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/artist"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="150dp"
        android:layout_marginLeft="150dp"
        android:text="TextView"
        android:textSize="16sp"/>

    <TextView
        android:id="@+id/venue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/artist"
        android:layout_alignLeft="@+id/artist"
        android:layout_alignParentTop="true"
        android:layout_marginTop="70dp"
        android:text="Venue"
        android:textStyle="bold"
        android:textSize="16sp"/>

    <TextView
        android:id="@+id/venueVal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/artistVal"
        android:layout_alignLeft="@id/artistVal"
        android:layout_alignTop="@+id/venue"
        android:text="TextView"
        android:textSize="16sp"/>

    <TextView
        android:id="@+id/time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/artist"
        android:layout_alignLeft="@id/artist"
        android:layout_alignParentTop="true"
        android:layout_marginTop="110dp"
        android:text="Time"
        android:textStyle="bold"
        android:textSize="16sp"/>

    <TextView
        android:id="@+id/timeVal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/artistVal"
        android:layout_alignLeft="@+id/artistVal"
        android:layout_alignBottom="@+id/time"
        android:text="TextView"
        android:textSize="16sp"/>

    <TextView
        android:id="@+id/category"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/artist"
        android:layout_alignLeft="@id/artist"
        android:layout_alignParentTop="true"
        android:layout_marginTop="150dp"
        android:text="Category"
        android:textStyle="bold"
        android:textSize="16sp"/>

    <TextView
        android:id="@+id/categoryVal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/artistVal"
        android:layout_alignLeft="@id/artistVal"
        android:layout_alignTop="@+id/category"
        android:text="TextView"
        android:textSize="16sp"/>

    <!--<TextView-->
        <!--android:id="@+id/priceRange"-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:layout_alignStart="@+id/artist"-->
