    <TextView
        android:id="@+id/ticketStatus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/artist"
        android:layout_alignLeft="@id/artist"
        android:layout_alignParentTop="true"
        android:layout_marginTop="190dp"
        android:text="Ticket Status"
        android:textStyle="bold"
        android:textSize="16sp"/>

    <TextView
        android:id="@+id/ticketStatusVal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/ticketStatus"
        android:layout_alignStart="@+id/artistVal"
        android:layout_alignLeft="@id/artistVal"
        android:text="TextView"
        android:textSize="16sp"/>

    <TextView
        android:id="@+id/buyTicket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/artist"
        android:layout_alignLeft="@id/artist"
        android:layout_alignParentTop="true"
        android:layout_marginTop="230dp"
        android:text="Buy Ticket At"
        android:textStyle="bold"
        android:textSize="16sp"/>

    <TextView
        android:id="@+id/buyTicketVal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/artistVal"
        android:layout_alignLeft="@id/artistVal"
        android:layout_alignTop="@+id/buyTicket"
        android:text="TextView"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/seatmap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/artist"
        android:layout_alignLeft="@id/artist"
        android:layout_alignParentTop="true"
        android:layout_marginTop="270dp"
        android:text="SeatMap"
        android:textStyle="bold"
        android:textSize="16sp"/>

    <TextView
        android:id="@+id/seatmapVal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/artistVal"
        android:layout_alignLeft="@id/artistVal"
        android:layout_alignBottom="@+id/seatmap"
        android:text="TextView"
        android:textSize="16sp"/>

</RelativeLayout>
    <LinearLayout
        android:id="@+id/eventNoResults"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <ProgressBar
        android:id="@+id/progressBar3"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity = "center"
        android:visibility="gone" />
    </LinearLayout>

</RelativeLayout>


